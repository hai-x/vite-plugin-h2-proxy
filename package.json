{
  "name": "vite-plugin-h2-proxy",
  "version": "1.0.0",
  "description": "",
  "author": "haijie-x",
  "keywords": [
    "vite",
    "proxy",
    "http2",
    "plugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/haijie-x/vite-plugin-h2-proxy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haijie-x/vite-plugin-h2-proxy.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm run link && pnpm run ts-watch",
    "link": "node ./scripts/link.mjs",
    "ts-watch": "tsup src/index.ts --format esm --watch",
    "lint": "eslint . --fix",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm run lint && pnpm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "http2-proxy": "^5.0.53"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.8.0",
    "execa": "^6.1.0",
    "prettier": "^2.8.0",
    "tsx": "^3.12.1",
    "typescript": "^4.9.3",
    "vite": "^4.4.9",
    "tsup": "^6.7.0"
  }
}
